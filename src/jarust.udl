namespace jarust {
    void init_logger();
};

dictionary JaConfig {
    string uri;
    string? apisecret;
    string? root_namespace;
};

interface JaConnection {
    [Throws=JaError]
    constructor();
    void connect(JaConfig config, JaCallback cb);
};

callback interface JaCallback {
    void on_connection_success();
    void on_connection_failure();
};

[Error]
enum JaError {
    "RuntimeCreationFailure",
};
