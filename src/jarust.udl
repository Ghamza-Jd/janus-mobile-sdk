namespace jarust {
    void init_logger();
};

dictionary RawJaConfig {
    string uri;
    string? apisecret;
    string? root_namespace;
};

interface RawJaConnection {
    [Throws=RawJaError]
    constructor();
    void connect(RawJaConfig config, RawJaCallback cb);
};

callback interface RawJaCallback {
    void on_connection_success();
    void on_connection_failure();
};

[Error]
enum RawJaError {
    "RuntimeCreationFailure",
};
